{
  "name": "json-server",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:client": "ng serve --aot",
    "start:server": "ts-node -r tsconfig-paths/register -P server/tsconfig.build.json server/src/main.ts",
    "build": "run-p build:client build:server",
    "build:client": "ng build --aot",
    "build:server": "tsc -p server/tsconfig.build.json",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"server/src/**/*.ts\" \"server/test/**/*.ts\"",
    "test:watch": "jest --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.0",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.0",
    "@angular/compiler": "~8.2.0",
    "@angular/core": "~8.2.0",
    "@angular/forms": "~8.2.0",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.0",
    "@angular/platform-browser-dynamic": "~8.2.0",
    "@angular/router": "~8.2.0",
    "@briebug/jest-schematic": "^2.1.1",
    "@nestjs/common": "^6.7.2",
    "@nestjs/config": "^0.1.0",
    "@nestjs/core": "^6.7.2",
    "@nestjs/platform-express": "^6.7.2",
    "@nestjs/serve-static": "^1.0.2",
    "@ngrx/effects": "^8.4.0",
    "@ngrx/store": "^8.4.0",
    "@types/mongodb": "^3.3.15",
    "hammerjs": "^2.0.8",
    "mongodb": "^3.5.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.24",
    "@angular/cli": "~8.2.0",
    "@angular/compiler-cli": "~8.2.0",
    "@angular/language-service": "~8.2.0",
    "@nestjs/cli": "^7.0.2",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.7.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.24",
    "@types/node": "~8.9.4",
    "@types/supertest": "^2.0.8",
    "codelyzer": "^5.0.0",
    "eslint": "^6.6.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^25.1.0",
    "jest-preset-angular": "8.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "protractor": "~5.4.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "ts-node": "~7.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  },
  "resolutions": {
    "serialize-javascript": "2.1.1"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$",
        "astTransformers": [
          "jest-preset-angular/build/InlineFilesTransformer",
          "jest-preset-angular/build/StripStylesTransformer"
        ]
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "preset": "jest-preset-angular",
    "moduleNameMapper": {
      "@app/(.*)": "<rootDir>/src/app/$1",
      "@assets/(.*)": "<rootDir>/src/assets/$1",
      "@core/(.*)": "<rootDir>/src/app/core/$1",
      "@env": "<rootDir>/src/environments/environment",
      "@src/(.*)": "<rootDir>/src/src/$1",
      "@state/(.*)": "<rootDir>/src/app/state/$1"
    },
    "rootDir": "src",
    "roots": [
      "src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup-jest.ts"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(ts|js|html)$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
